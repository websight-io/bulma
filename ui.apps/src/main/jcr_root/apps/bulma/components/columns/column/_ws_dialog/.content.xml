          xmlns:nt="http://www.jcp.org/jcr/nt/1.0" xmlns:ws="http://ds.pl/websight"
          jcr:primaryType="nt:unstructured"
          sling:resourceType="wcm/dialogs/dialog">
    <tabs
            jcr:primaryType="nt:unstructured"
            sling:resourceType="wcm/dialogs/components/tabs">
        <mobileStyleTab
                jcr:primaryType="nt:unstructured"
                sling:resourceType="wcm/dialogs/components/tab"
                label="Mobile column style">
            <isNarrowColumn
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/dialogs/components/toggle"
                    name="mobileColumnStyle/isNarrowColumn"
                    label="Narrow column"
                    description="Enable this, so the column will only take the actual space it needs."
            />
            <widthContainer
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="wcm/dialogs/components/container">
                <sizeType
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/dialogs/components/include">
                    <include
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/bulma/components/columns/attributes/type"
                            name="mobileColumnStyle/sizeType"
                            label="Type of the column size">
                    </include>
                </sizeType>
                <evenNumber
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/dialogs/components/container">
                    <size
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/dialogs/components/include">
                        <include
                                jcr:primaryType="nt:unstructured"
                                sling:resourceSuperType="/apps/bulma/components/columns/attributes/evennumber"
                                name="mobileColumnStyle/size"
                                label="Column size">
                        </include>
                    </size>
                    <ws:display
                            jcr:primaryType="nt:unstructured">
                        <condition
                                jcr:primaryType="nt:unstructured"
                                sourceName="mobileColumnStyle/sizeType"
                                values="evenNumber"/>
                    </ws:display>
                </evenNumber>
                <fraction
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/dialogs/components/container">
                    <style
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/dialogs/components/include">
                        <include
                                jcr:primaryType="nt:unstructured"
                                sling:resourceSuperType="/apps/bulma/components/columns/attributes/fraction"
                                name="mobileColumnStyle/size"
                                label="Column size">
                        </include>
                    </style>
                    <ws:display
                            jcr:primaryType="nt:unstructured">
                        <condition
                                jcr:primaryType="nt:unstructured"
                                sourceName="mobileColumnStyle/sizeType"
                                values="fraction"/>
                    </ws:display>
                </fraction>
                <offsetType
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/dialogs/components/include">
                    <include
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/bulma/components/columns/attributes/type"
                            name="mobileColumnStyle/offsetType"
                            label="Type of the offset size">
                    </include>
                </offsetType>
                <offsetEvenNumber
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/dialogs/components/container">
                    <size
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/dialogs/components/include">
                        <include
                            jcr:primaryType="nt:unstructured"
                            sling:resourceSuperType="/apps/bulma/components/columns/attributes/evennumber"
                            name="offset"
                            label="Offset size"
                            sling:hideChildren="[twelve]">
                        </include>
                    </size>
                    <ws:display
                            jcr:primaryType="nt:unstructured">
                        <condition
                                jcr:primaryType="nt:unstructured"
                                sourceName="mobileColumnStyle/offsetType"
                                values="evenNumber"/>
                    </ws:display>
                </offsetEvenNumber>
                <offsetFraction
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="wcm/dialogs/components/container">
                    <style
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="wcm/dialogs/components/include">
                        <include
