                                sling:resourceSuperType="/apps/bulma/components/columns/attributes/fraction"
                                name="mobileColumnStyle/offset"
                                label="Offset size"
                                sling:hideChildren="[full]">
                        </include>
                    </style>
                    <ws:display
                            jcr:primaryType="nt:unstructured">
                        <condition
                                jcr:primaryType="nt:unstructured"
                                sourceName="mobileColumnStyle/offsetType"
                                values="fraction"/>
                    </ws:display>
